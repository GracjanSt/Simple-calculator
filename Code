print("Calculator")

print("Choose your Destiny ")
wybor = input("""
1 - addition, 2 - substraction,
3 - division, 4 - multiplication,
5 - exponentation, 6 - modulo
""")

a = int(input("Choose first number: "))
b = int(input("Choose second number: "))

if(wybor == '1'):
    print (a + b)
elif(wybor == '2' ):
    print (a - b)
elif(wybor == '3'):
    if(b == 0):
        print("Do not divide by zero")
    else:
        print(a / b)
elif(wybor == '4'):
    print(a * b)
elif(wybor == '5'):
    print(a ** b)
elif(wybor == '6'):
    print(a % b)
